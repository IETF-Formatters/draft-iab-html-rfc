      <optional>
        <attribute name="consensus" a:defaultValue="false">
          <choice>
            <value datatypeLibrary="http://www.w3.org/2001/XMLSchema-datatypes">no</value>
            <value datatypeLibrary="http://www.w3.org/2001/XMLSchema-datatypes">yes</value>
            <value datatypeLibrary="http://www.w3.org/2001/XMLSchema-datatypes">false</value>
            <value datatypeLibrary="http://www.w3.org/2001/XMLSchema-datatypes">true</value>
          </choice>
        </attribute>
      </optional>
      <optional>
        <attribute name="seriesNo"/>
      </optional>
      <optional>
        <attribute name="ipr"/>
      </optional>
      <optional>
        <attribute name="iprExtract">
          <data type="IDREF" datatypeLibrary="http://www.w3.org/2001/XMLSchema-datatypes"/>
        </attribute>
      </optional>
      <optional>
        <attribute name="submissionType" a:defaultValue="IETF">
          <choice>
            <value datatypeLibrary="http://www.w3.org/2001/XMLSchema-datatypes">IETF</value>
            <value datatypeLibrary="http://www.w3.org/2001/XMLSchema-datatypes">IAB</value>
            <value datatypeLibrary="http://www.w3.org/2001/XMLSchema-datatypes">IRTF</value>
            <value datatypeLibrary="http://www.w3.org/2001/XMLSchema-datatypes">independent</value>
          </choice>
        </attribute>
      </optional>
      <optional>
        <attribute name="docName"/>
      </optional>
      <optional>
        <attribute name="sortRefs" a:defaultValue="false">
           <choice>
            <value datatypeLibrary="http://www.w3.org/2001/XMLSchema-datatypes">true</value>
            <value datatypeLibrary="http://www.w3.org/2001/XMLSchema-datatypes">false</value>
          </choice>
        </attribute>
      </optional>
      <optional>
        <attribute name="symRefs" a:defaultValue="true">
           <choice>
            <value datatypeLibrary="http://www.w3.org/2001/XMLSchema-datatypes">true</value>
            <value datatypeLibrary="http://www.w3.org/2001/XMLSchema-datatypes">false</value>
          </choice>
        </attribute>
      </optional>
      <optional>
        <attribute name="tocInclude" a:defaultValue="true">
           <choice>
            <value datatypeLibrary="http://www.w3.org/2001/XMLSchema-datatypes">true</value>
            <value datatypeLibrary="http://www.w3.org/2001/XMLSchema-datatypes">false</value>
          </choice>
        </attribute>
      </optional>
      <optional>
        <attribute name="tocDepth" a:defaultValue="3"/>
      </optional>
      <optional>
        <attribute name="prepTime"/>
      </optional>
      <optional>
        <attribute name="indexInclude" a:defaultValue="true">
           <choice>
            <value datatypeLibrary="http://www.w3.org/2001/XMLSchema-datatypes">true</value>
            <value datatypeLibrary="http://www.w3.org/2001/XMLSchema-datatypes">false</value>
          </choice>
        </attribute>
      </optional>
      <optional>
        <attribute name="version"/>
      </optional>
      <optional>
        <attribute name="scripts" a:defaultValue="Common,Latin"><?hidden-in-prose?></attribute>
      </optional>
      <optional>
        <attribute name="expiresDate"><?hidden-in-prose?></attribute>
      </optional>
      <zeroOrMore>
        <ref name="link"/>
      </zeroOrMore>
      <ref name="front"/>
      <ref name="middle"/>
      <optional>
        <ref name="back"/>
      </optional>
    </element>
  </define>
